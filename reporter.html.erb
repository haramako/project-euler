<html>
<head>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css" integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb" crossorigin="anonymous">
</head>
<body>

<h1>Project Euler Report</h1>
    
<table class="table table-bordered table-striped">
  <thead class="thead-dark">
  <tr>
    <th>問題</th>
    <% names.each do |name| %>
    <th><%= name %></th>
    <% end %>
  </tr>
  </thead>
  <tbody>
  <tr>
    <td>合計時間</td>
    <% reports.each do |r|%>
    <td><%= '%3.3f' % r.total_run_time %></td>
    <% end %>
  </tr>
  <tr>
    <td>正解数</td>
    <% reports.each do |r|%>
    <td><%= '%d' % [r.success_count] %></td>
    <% end %>
  </tr>
  <tr>
    <td></td>
  </tr>
  <% (1..MAX).each do |n|%>
  <tr>
    <td><%= n %></td>
    <% med = median(reports.map{|r| r.list[n][:run_time]}) %>
    <% reports.each do |r|%>
    <%=show_report r.list[n], med %>
    <% end %>
  </tr>
  <% end %>
  </tbody>
</table>
